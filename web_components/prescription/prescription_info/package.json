{
  "name": "@rx/micro-prescription-info",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "node esbuild.config.mjs",
    "watch": "node esbuild.config.mjs --watch",
    "clean": "rm -rf dist",
    "pack": "npm run clean && npm install && npm run build && npm pack --pack-destination ../../temp_pack"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "typescript": "^5.4.0"
  }
}

